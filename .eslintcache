']} />\n        <span className={styles.filters__name}>3 пересадки</span>\n      </label>\n    </section>\n  );\n};\n\nconst mapStateToProps = (state: InitialStateType) => {\n  return {\n    props: state.transfer,\n  };\n};\n\nconst connector = connect(mapStateToProps, actions);\n\nexport default connector(Filters);\n","/home/kirill/react-apps/aviasales/src/components/Tab/Tab.tsx",[],"/home/kirill/react-apps/aviasales/src/components/TicketList/TicketList.tsx",[],"/home/kirill/react-apps/aviasales/src/components/Tab/index.ts",[],"/home/kirill/react-apps/aviasales/src/components/Ticket/index.ts",[],"/home/kirill/react-apps/aviasales/src/redux/reducer.ts",[],"/home/kirill/react-apps/aviasales/src/components/Content/index.ts",[],"/home/kirill/react-apps/aviasales/src/components/Filters/index.ts",[],"/home/kirill/react-apps/aviasales/src/redux/action.ts",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"93","replacedBy":"105"},{"ruleId":"95","replacedBy":"106"},{"ruleId":"97","replacedBy":"107"},{"ruleId":"99","replacedBy":"108"},{"ruleId":"101","replacedBy":"109"},{"ruleId":"103","replacedBy":"110"},{"ruleId":"93","replacedBy":"111"},{"ruleId":"95","replacedBy":"112"},{"ruleId":"97","replacedBy":"113"},{"ruleId":"99","replacedBy":"114"},{"ruleId":"101","replacedBy":"115"},{"ruleId":"103","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":18,"column":11,"nodeType":"119","messageId":"120","endLine":18,"endColumn":23},"jsx-a11y/accessible-emoji",[],"lines-around-directive",["121"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],[],["121"],[],[],[],[],[],["121"],[],[],[],[],"@typescript-eslint/no-unused-vars","'isAllPressed' is assigned a value but never used.","Identifier","unusedVar","padding-line-between-statements"]